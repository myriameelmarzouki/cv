ition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-12\" mbr-if=\"showSubtitle || showTitle\">\n                <div class=\"title_block text-center\">\n                    <h4 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-subtitle\"><b>WE ARE THE BEST​</b></h4>\n                    <h3 class=\"mbr-section-title mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\"><b>Why Choose our Firm</b></h3>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-12 col-lg-12\">\n                <div class=\"row\">\n                    <div class=\"col-12 col-md-6 item\" mbr-class=\"{'col-lg-3' : cardCount > 3, 'col-lg-4' : cardCount < 4, 'col-lg-5' : cardCount < 3}\">\n                        <div class=\"card_wrapper text-center\">\n                            <div class=\"icon_block\" mbr-if=\"showIcon\">\n                                <div class=\"iconfont-wrapper\">\n                                    <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 icon54-v1-business-woman\"></span>\n                                    <div class=\"shape_1 shape\"></div>\n                                    <div class=\"shape_2 shape\"></div>\n                                    <div class=\"shape_3 shape\"></div>\n                                    <div class=\"shape_4 shape\"></div>     